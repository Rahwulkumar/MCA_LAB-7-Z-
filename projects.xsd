<?xml version="1.0" encoding="UTF-8"?>
    <xs:element name="projects">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="project" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="title" type="xs:string"/>
                            <xs:element name="description" type="xs:string"/>
